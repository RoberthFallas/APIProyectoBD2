<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="1.0">
                    
    
    
    
    <persistence-unit name="Bd2PU" transaction-type="JTA">
                                    
        
        
        
        <jta-data-source>jdbc/BDDataSource</jta-data-source>
                                    
        
        
        
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
                                    
        
        
        
        <properties name="eclipselink.logging.level.sql" value="FINE"/>
                                    
        
        
        
        <properties name="eclipselink.logging.paramerters" value="true"/>
                                    
        
        
        
        <properties name="eclipselink.logging.logger" value="JavaLogger"/>
                                    
        
        
        
        <properties name="eclipselink.logging.level" value="FINE"/>
                                  
    
    
    
    </persistence-unit>
                



</persistence>
